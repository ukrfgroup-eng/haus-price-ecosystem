# Настройки Flask
FLASK_ENV=development
FLASK_DEBUG=True
SECRET_KEY=your-secret-key-here

# База данных
DATABASE_URL=sqlite:///app.db

# Внешние API (для будущих интеграций)
UMNIKO_API_KEY=your-umniko-api-key
TILDA_API_KEY=your-tilda-api-key

# Настройки бота
BOT_WEBHOOK_URL=https://your-domain.com/webhook
BOT_TIMEOUT=30

# Логирование
LOG_LEVEL=INFO
LOG_FILE=app.log
